<?xml version="1.0" encoding="utf-8"?>
<shape xmlns:android="http://schemas.android.com/apk/res/android">
    <!-- Background color of the EditText -->
    <solid android:color="#FFFFFF" />

    <!-- Border around the EditText -->
    <stroke
        android:width="1dp"
        android:color="#0E983D" />

    <!-- Rounded corners -->
    <corners android:radius="8dp" />

    <!-- Padding inside the EditText -->
    <padding
        android:left="10dp"
        android:top="10dp"
        android:right="10dp"
        android:bottom="10dp" />
</shape>
